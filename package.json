{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -H 0.0.0.0 -p 3000",
    "dev:clear": "next dev --turbopack -H 0.0.0.0 -p 3000 --clear",
    "dev:replit": "next dev --turbopack -H 0.0.0.0 -p 3000",
    "setup-local-ip": "node setup-local-ip.js",
    "dev:auto-restart": "node auto-restart-dev.js",
    "tunnel": "node start-cloudflare-tunnel.js",
    "tunnel:setup": "node setup-cloudflare-url.js",
    "build": "next build",
    "build:debug": "next build --debug",
    "start": "cross-env PORT=${PORT:-3000} next start -H 0.0.0.0 -p ${PORT:-3000}",
    "start:prod": "cross-env NODE_ENV=production PORT=${PORT:-3000} next start -H 0.0.0.0 -p ${PORT:-3000}",
    "start:windows": "next start -p 3000",
    "start:replit": "npm run build && npm run start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "test:full": "npm run lint && npm run typecheck",
    "deploy": "./deploy.sh",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop whitepointer-app",
    "pm2:restart": "pm2 restart whitepointer-app",
    "pm2:logs": "pm2 logs whitepointer-app",
    "backup-db": "echo 'Use Neon backups or pg_dump'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "setup:postgresql": "node scripts/setup-postgresql.js",
    "verify:postgresql": "node scripts/verify-postgresql.js",
    "db:health": "curl -s http://localhost:3000/api/health | jq ."
  },
